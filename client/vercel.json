{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": { 
        "distDir": "dist"
      }
    }
  ],
  "buildCommand": "npm run vercel-build",
  "installCommand": "npm ci",
  "functions": {
    "app/api/**/*.js": {
      "runtime": "nodejs16.x"
    }
  }
}